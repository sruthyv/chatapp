<p>
  <h3>
    <%= form_for (:session) do |f| %>
      <%= f.label :username, 'Enter your username' %>
      <%= f.text_field :username, autocomplete: 'off' %>
      <hr>
      <%= f.label :email, 'Enter your email' %>
      <%= f.text_field :email, autocomplete: 'off' %>
      <hr>
      <%= f.submit 'Join ChatApp' %>
    <% end %>
  </h3>
</p>
